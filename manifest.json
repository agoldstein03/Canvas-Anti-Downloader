{
  "manifest_version": 2,
  "name": "Canvas Anti-Downloader",
  "short_name": "CAD",
  "version": "1.0.0",
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "minimum_chrome_version": "40",
  "description": "Simple extension that adds an option on Canvas to preview download links.",
  "background": {
    "scripts": ["content.js"]
  }
}
